  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "build:prod:win": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode production",
    "build:prod:unix": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode production",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode production",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src"
  },
